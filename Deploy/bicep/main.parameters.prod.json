{
	"$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"storageAccountSkuName": {
			"value": "Standard_LRS"
		},
		"StorageAccountsArr": {
			"value": [ "st", "staudit", "cts" ]
		},
		"appServicePlanSku": {
			"value": {
				"name": "S1",
				"tier": "Standard",
				"size": "S1",
				"family": "S",
				"capacity": "2"
			}
