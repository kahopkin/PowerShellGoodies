				"softDeleteRetentionInDays": 7,
				"enableRbacAuthorization": false,
				"enablePurgeProtection": true,
				"skuName": "premium",
				"family": "A",
				"keys": [
					"Get",
					"List",
					"Update",
					"Create",
					"Import",
					"Delete",
					"Recover",
					"Backup",
					"Restore",
