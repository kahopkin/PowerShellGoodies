# ---Create React App Variables (https://create-react-app.dev/docs/advanced-configuration/)---

# GENERATE_SOURCEMAP: Whether or not the build process should generate that js.map files used to show source code
GENERATE_SOURCEMAP=false

# ---Custom Environment Variables---
# Custom Environment Variables need to be prefixed with REACT_APP
# For custom env variables that store arrays, enter values separated by a comma with no spaces after. For example, TEST_VAR=a,b,c

# REACT_APP_AAD_CLIENT_ID: Client Id for Azure AD
# This is the client id of the DPP UI's App registration
# Azure Location: App Registrations, Client Web App, Application (client) ID
REACT_APP_AAD_CLIENT_ID=<DPPUIClientId>

# REACT_APP_AAD_AUTHORITY: Authority for Azure AD
# Syntax is usually: https://<AAD base url>/<TenantId>
# ex: https://login.microsoftonline.us/<tenantId>
# Azure Location: AAD, Tenant ID
REACT_APP_AAD_AUTHORITY=<AADAuthorityUrl>

# REACT_APP_AAD_REDIRECT_URI: Redirect Uri for Azure AD
# This the url that AAD will redirect to after the user successfully logs in.
# Should be the website's root url
# Azure Location: App Service, URL
REACT_APP_AAD_REDIRECT_URI=<DPPUIWebsiteUrl>

# REACT_APP_AAD_SCOPES: Scopes used when logging in. Inputs follow the defined array convention
REACT_APP_LOGIN_SCOPES=array:User.Read

# REACT_APP_GRAPH_ENDPOINT: Endpoint in which graph requests will be made against
# This is the endpoint used to hit for the graph api.
# Ensure it matches the appropiate environment where the code is deployed. (commerical, govt, ex, rx, etc.)
# ex: https://graph.microsoft.us/v1.0/me
# Azure Location: App Service, URL
REACT_APP_GRAPH_ENDPOINT=<GraphEndpointUrl>

# REACT_APP_GRAPH_SCOPES: Scopes used when making a request to the graph api. Inputs follow the defined array convention
REACT_APP_GRAPH_SCOPES=array:User.Read

# REACT_APP_DPP_API_ENDPOINT: Endpoint in which DPP api requests will be made against
# This is the url of the DPP API function app. 
REACT_APP_DPP_API_ENDPOINT=<DPPApiFunctionAppUrl>

# REACT_APP_DPP_API_SCOPES: Scopes used when making a request to the DPP api. Inputs follow the defined array convention
# This is the scope that the code will use when requesting access tokens to the api app.
# Syntax is usually: array:api://<DPPFunctionAppRegistrationClientId>/.default
# Azure Location: API App Registration, Application ID URI
REACT_APP_DPP_API_SCOPES=<DPPApiScopes>

# REACT_APP_COMPLETED_TRANFERS_POLLING_INTERVAL_MS: Time, in Milliseconds, that the polling on the completed transfers screen will fire
REACT_APP_COMPLETED_TRANFERS_POLLING_INTERVAL_MS=10000

REACT_APP_HELP_URL="https://app-docfx.azurewebsites.us"

# REACT_APP_VALIDATION_CHECK_INTERVAL: Optional: The interval of how often the application will request validation of pending transfer assignments.
REACT_APP_VALIDATION_CHECK_INTERVAL = 60000